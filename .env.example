# Expo Configuration
EXPO_NO_CAPABILITY_SYNC=1

# Supabase
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# RevenueCat (automatically set by Vibecode when configured in Payments tab)
# Test key used in development/test builds (both platforms)
EXPO_PUBLIC_VIBECODE_REVENUECAT_TEST_KEY=your-revenuecat-test-key
# Apple key used in production builds (iOS)
EXPO_PUBLIC_VIBECODE_REVENUECAT_APPLE_KEY=your-revenuecat-apple-key
# Google key used in production builds (Android)
EXPO_PUBLIC_VIBECODE_REVENUECAT_GOOGLE_KEY=your-revenuecat-google-key

# OpenAI (for AI features)
EXPO_PUBLIC_OPENAI_API_KEY=your-openai-api-key

# Feature Flags
EXPO_PUBLIC_TRUST_FILTER_ENABLED=true
EXPO_PUBLIC_TRUST_FILTER_TRACE_ENABLED=true
EXPO_PUBLIC_STYLE_SIGNALS_ENABLED=true
EXPO_PUBLIC_LAZY_ENRICHMENT_ENABLED=true

# AI Safety Check (client-side flags)
EXPO_PUBLIC_AI_SAFETY_ENABLED=true
EXPO_PUBLIC_AI_SAFETY_DRY_RUN=true
EXPO_PUBLIC_AI_SAFETY_ROLLOUT_PCT=10

# Analytics (force DB writes in dev)
EXPO_PUBLIC_FORCE_ANALYTICS_DB=false

# ─────────────────────────────────────────────
# SUPABASE EDGE FUNCTION ENV VARS
# ─────────────────────────────────────────────
# These are set via Supabase Dashboard > Edge Functions > Secrets
# Documented here for reference:
#
# AI_SAFETY_ENABLED=true          # Enable/disable AI Safety Check
# AI_SAFETY_DRY_RUN=true          # Log verdicts but don't apply (for testing)
# AI_SAFETY_TIMEOUT_MS=1000       # Timeout for AI calls (ms)
# AI_SAFETY_DAILY_CAP=50          # Max AI calls per user per day
# AI_SAFETY_MODEL_ID=gpt-4o       # Model to use (gpt-4o, gpt-4o-mini)
# AI_SAFETY_PROMPT_VERSION=1      # Prompt version for cache invalidation
