# Expo Configuration
EXPO_NO_CAPABILITY_SYNC=1

# Supabase
EXPO_PUBLIC_SUPABASE_URL=your-supabase-url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key

# RevenueCat
EXPO_PUBLIC_REVENUECAT_API_KEY=your-revenuecat-api-key

# OpenAI (for AI features)
EXPO_PUBLIC_OPENAI_API_KEY=your-openai-api-key

# Feature Flags
EXPO_PUBLIC_TRUST_FILTER_ENABLED=true
EXPO_PUBLIC_TRUST_FILTER_TRACE_ENABLED=true
EXPO_PUBLIC_STYLE_SIGNALS_ENABLED=true
EXPO_PUBLIC_LAZY_ENRICHMENT_ENABLED=true

# Analytics (force DB writes in dev)
EXPO_PUBLIC_FORCE_ANALYTICS_DB=false
